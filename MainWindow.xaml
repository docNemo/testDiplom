<Window x:Class="testDiplom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testDiplom"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="input" HorizontalAlignment="Left" Margin="118,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="483" TextChanged="TextBox_TextChanged"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="626,37,0,0" VerticalAlignment="Top" Height="43" Width="105" Click="Button_Click"/>
        <DataGrid x:Name="results" Margin="0,217,0,0">
            AutoGenerateColumns="False"
                DockPanel.Dock="Top"
            <DataGrid.Columns>
                <DataGridTextColumn
                        Width="*"
                        Header="Вывод"
                        Binding="{Binding Path=Value}"
                        IsReadOnly="True" />
            </DataGrid.Columns>
        </DataGrid>
        <TextBox x:Name="output" HorizontalAlignment="Left" Margin="118,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="483"/>

    </Grid>
</Window>
