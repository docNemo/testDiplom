<Window x:Class="testDiplom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:testDiplom"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,119,0">
        <TextBox x:Name="input" HorizontalAlignment="Left" Margin="118,90,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="483" TextChanged="TextBox_TextChanged"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="307,136,0,0" VerticalAlignment="Top" Height="43" Width="105" Click="Button_Click"/>
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,217,-118,0"/>

    </Grid>
</Window>
